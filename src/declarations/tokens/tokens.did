type UserId = principal;
type TokenType = 
 variant {
   copyright;
   governance;
 };
type NewToken = 
 record {
   recordId: text;
   symbol: text;
   tokenType: TokenType;
   totalSupply: nat;
 };
service : {
  addRecord: (text) -> (bool);
  createNewToken: (NewToken) -> (nat32);
  initializeEmptyValuesForUser: (UserId) -> ();
}
